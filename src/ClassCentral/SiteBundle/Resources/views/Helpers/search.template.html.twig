<!-- Handlebars template scripts for Typeahead-->
{% raw %}
<script id="course-template" type="text/x-handlebars-template">
    <a class="type-{{payload.type}} suggestion-template template-course" href="{{payload.url}}" data-type="{{payload.type}}" data-name="{{payload.name}}" onmousedown="ga('send','event', 'Search Autocomplete' , '{{payload.type}}', '{{payload.name}}' )">
        <span class="name">{{payload.name}}</span>
        <div class="session-wrap">
            {{#if payload.nextSession}}
            <span class="next-session">{{payload.nextSession}}</span>
            {{/if}}
            {{#if payload.provider}}
            <span class="course-provider">&nbsp;via {{payload.provider}}</span>
            {{/if}}
            <span class="session_hoverMeta" data-href="#">
                <span class="session_hoverMetaIcon">
                    <div class="u-scaling-svg">
                        <svg class="u-scalingSvg__shape">
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#shape-direct-link"></use>
                        </svg>
                    </div>
                </span>
                <span class="session_hoverMetaText">Direct to class</span>
            </span>
        </div>
    </a>
</script>

